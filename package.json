{
	"name": "futurebank",
	"version": "1.0.0",
	"description": "Lloc web modern per FutureBank",
	"main": "index.js",
	"scripts": {
		"lint": "eslint . --config eslint.config.mjs",
		"lint:fix": "eslint **/*.js --fix",
		"proxy": "node src/utils/proxy-server/proxy-server.js",
		"test": "npx playwright test",
		"test:ui": "npx playwright test --ui",
		"test:headed": "npx playwright test --headed",
		"test:debug": "npx playwright test --debug",
		"test:report": "npx playwright show-report",
		"test:install": "npx playwright install chromium",
		"test:install-all": "npx playwright install",
		"test:ci": "npx playwright test --reporter=github",
		"test:watch": "npx playwright test --watch",
		"test:grep": "npx playwright test --grep",
		"test:failed": "npx playwright test --last-failed",
		"test:list": "npx playwright test --list",
		"test:trace": "npx playwright test --trace on",
		"test:video": "npx playwright test --video on",
		"test:mobile": "npx playwright test --project='Mobile Chrome'",
		"test:desktop": "npx playwright test --project='Desktop Chrome'",
		"test:smoke": "npx playwright test tests/01-initial-load.spec.js",
		"test:accessibility": "npx playwright test tests/08-accessibility.spec.js",
		"test:performance": "npx playwright test tests/10-performance.spec.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "module",
	"devDependencies": {
		"@eslint/eslintrc": "^2.1.4",
		"@eslint/js": "^9.4.0",
		"@playwright/test": "^1.55.1",
		"cssnano": "^7.0.6",
		"eslint": "^9.4.0",
		"http-server": "^14.1.1",
		"terser": "^5.39.0"
	},
	"dependencies": {
		"chart.js": "^4.4.9",
		"chartjs-chart-financial": "^0.2.1",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"express": "^5.1.0",
		"globals": "^16.4.0",
		"node-fetch": "^3.3.2",
		"uuid": "^13.0.0"
	}
}
